{
  "name": "audio-extender",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c && npm run prepare",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepare": "mkdir public/build && npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/build/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/build/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@material/typography": "^13.0.0",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.0.0",
    "@smui/button": "^6.0.0-beta.15",
    "@smui/card": "^6.0.0-beta.15",
    "@smui/circular-progress": "^6.0.0-beta.15",
    "@smui/common": "^6.0.0-beta.15",
    "@smui/dialog": "^6.0.0-beta.15",
    "@smui/form-field": "^6.0.0-beta.15",
    "@smui/icon-button": "^6.0.0-beta.15",
    "@smui/layout-grid": "^6.0.0-beta.15",
    "@smui/list": "^6.0.0-beta.15",
    "@smui/menu": "^6.0.0-beta.15",
    "@smui/menu-surface": "^6.0.0-beta.15",
    "@smui/select": "^6.0.0-beta.16",
    "@smui/slider": "^6.0.0-beta.15",
    "@smui/snackbar": "^6.0.0-beta.16",
    "@smui/textfield": "^6.0.0-beta.15",
    "@smui/top-app-bar": "^6.0.0-beta.15",
    "@tsconfig/svelte": "^3.0.0",
    "@types/audiobuffer-to-wav": "^1.0.1",
    "rollup": "^2.3.4",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "smui-theme": "^6.0.0-beta.15",
    "svelte": "^3.0.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@fontsource/roboto": "^4.5.3",
    "@material/button": "^13.0.0",
    "@material/top-app-bar": "^13.0.0",
    "@mdi/js": "^6.6.95",
    "audiobuffer-to-wav": "^1.0.0",
    "idb": "^7.0.0",
    "sirv-cli": "^2.0.2",
    "svelte-writable-derived": "^2.1.3"
  }
}
